{
    "name": "szaturn",
    "version": "0.1.0",
    "description": "Szaturn for Neptun",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "*://*.elte.hu/*",
        "activeTab",
        "unlimitedStorage",
        "cookies",
        "webRequest",
        "webRequestBlocking"
    ]
}